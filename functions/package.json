{
  "name": "functions",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts . --fix",
    "format": "prettier --write .",
    "build": "tsc",
    "dev": "tsc-watch",
    "serve": "firebase emulators:start --only functions,firestore",
    "shell": "firebase functions:config:get > .runtimeconfig.json && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "fsemulator": "export JAVA_TOOL_OPTIONS='-Xmx8g';firebase emulators:start --only firestore --import=./firestore_emulator_data --export-on-exit"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.27.2",
    "common": "link:./common",
    "crypto": "^1.0.1",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^3.14.1",
    "unzipper": "^0.10.11",
    "xbrl-parser": "^0.6.0",
    "xml2js": "^0.4.23",
    "yahoo-finance2": "^2.3.6"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node-fetch": "^2.6.1",
    "@types/unzipper": "^0.10.5",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^2.2.0",
    "prettier": "2.6.2",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.2"
  },
  "private": true
}
